local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))() 

local Window = OrionLib:MakeWindow({Name = "N Vip Aura craft ", HidePremium = false,IntroText = "hey!", SaveConfig = true, ConfigFolder = "scripts"}) --intro y nombre de inicio pero no cambiar lo demas

--inicio ajustes Tab

OrionLib:MakeNotification({
	Name = "Hello!!",
	Content = "Owner scripts!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Auras",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "if you want the auras in the inv just rejoin to get them" 
})

--botones Tab

Tab:AddButton({
	Name = "black hole",
	Callback = function()
       	local args = {
    [1] = "black hole",
    [2] = "EEOQRFIWGEW!@#$@(#WRUREFJIGERJNPWQDLP#(!!!!"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ADDAURA"):FireServer(unpack(args))

local args = {
    [1] = "black hole",
    [2] = "1"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Equip_AURA"):FireServer(unpack(args))

local args = {
    [1] = "black hole",
    [2] = "howdytherehowareyoudoinglolidk1243"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Set_ASYNC"):FireServer(unpack(args))


  	end    
})


